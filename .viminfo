"8	LINE	0
	---
	- name: Download Nexus
	  get_url:
	    url: "https://download.sonatype.com/nexus/3/latest-unix.tar.gz"
	    dest: "/tmp/nexus.tar.gz"
	
	- name: Extract Nexus
	  ansible.builtin.archive:
	    src: "/tmp/nexus.tar.gz"
	    dest: "/opt/"
	    remote_src: yes
	    mode: '755'
	
	- name: Rename Nexus Directory
	  command: mv /opt/nexus-* /opt/nexus
	
	- name: Set Nexus User Permissions
	  ansible.builtin.file:
	    path: /opt/nexus
	    owner: nexus
	    group: nexus
	    recurse: yes
	
	- name: Copy Nexus Repository Configuration
	  template:
	    src: nexus.repo.j2
	    dest: /etc/apt/sources.list.d/nexus.list
	  notify:
	    - Update apt Cache
	
	- name: Install Nexus
	  apt:
	    name: nexus
	    state: present
	
	- name: Enable and Start Nexus Service
	  systemd:
	    name: nexus
	    enabled: yes
	    state: started
	
